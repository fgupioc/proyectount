	<parameter name="numSalida" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select m.fechaRegistro as fecha,m.numSalida as numMemo,m.numExpediente as numExp,m.numDocumento as numDoc,m.asunto,m.solicitante,
are.descripcion,m.referencia,m.cantidad,un.descripcion as unidad,c.descripcion as cabecera,
 concat(p.nombre,' ',p.apellidoPaterno,' ',p.apellidoMaterno) as encargado
 , a.nombre as autorizante ,a.especialidad,a.cargo,
 concat(pro.articulo,'- Modelo :',m.modelo,' - Marca :',mar.descripcion,' - Color : ',m.color,
 ' - otros : ',m.observacion,' - Nº Serie :',m.numSerie,' - Cod. UNT :',m.codigoUnt,' - Cod. Sane :',m.codigoSaneamiento
 ) as producto

from movimiento as m inner join
autorizante as a on a.id= autorizante_id
join personal as p on m.personal_id = p.id
join producto as pro on m.producto_id = pro.id
join marca as mar on pro.marca_id=mar.id 
join unidad as un on pro.unidad_id = un.id
join area as are on m.area_id = are.id
join cabecera c on m.numSalida = c.codigoMovimiento
where m.operacion = 'salida'  and m.numSalida = $P{numSalida}]]>
	</queryString>
